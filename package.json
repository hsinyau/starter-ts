{
  "name": "ts-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@10.11.0",
  "description": "My typescript library sterter",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/hsinyau/ts-starter#readme",
  "bugs": {
    "url": "https://github.com/hsinyau/ts-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsinyau/ts-starter.git"
  },
  "author": "Hsinyau <hi@hsinyau.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.12.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}
